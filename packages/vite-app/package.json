{
    "name": "@hgod-in-cv/vite-app",
    "version": "0.2.0-alpha.0",
    "description": "Vite App for Henrique Godinho's CV",
    "author": "Henrique Godinho <ola@hgod.in>",
    "type": "module",
    "scripts": {
        "build": "vite build --emptyOutDir",
        "dev": "vite dev",
        "get:data": "node .bin/getData.js",
        "prod": "pnpm build --emptyOutDir && pnpm server:prod",
        "server": "node --loader ts-node/esm ./server/index.ts",
        "server:dev": "pnpm server",
        "server:prod": "cross-env NODE_ENV=production pnpm server",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
        "test:cov": "pnpm test -- --coverage --coverageDirectory=\"tests/coverage\""
    },
    "dependencies": {
        "@radix-ui/react-checkbox": "^1.1.1",
        "@radix-ui/react-collapsible": "^1.1.0",
        "@radix-ui/react-scroll-area": "^1.1.0",
        "@radix-ui/react-slider": "^1.2.0",
        "@radix-ui/react-switch": "^1.1.0",
        "@radix-ui/react-toggle-group": "^1.1.0",
        "d3": "^7.9.0",
        "eslint": "^9.9.0",
        "eslint-plugin-react": "^7.35.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.11",
        "framer-motion": "^11.3.29",
        "jsonld": "^8.3.2",
        "node-fetch": "^3.3.2",
        "react": "^18.3.1",
        "react-accessible-treeview": "^2.9.1",
        "react-dom": "^18.3.1",
        "react-feather": "^2.0.10",
        "react-force-graph-3d": "^1.24.3",
        "react-helmet": "^6.1.0",
        "three": "^0.167.1",
        "three-spritetext": "^1.8.2",
        "ts-node": "^10.9.2",
        "vike": "^0.4.191",
        "vike-react": "^0.5.3",
        "vite": "^5.4.2"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@types/d3": "^7.4.3",
        "@types/jsonld": "^1.5.15",
        "@types/node": "^22.5.0",
        "@types/react": "^18.3.4",
        "@types/react-dom": "^18.3.0",
        "@types/react-helmet": "^6.1.11",
        "@types/three": "^0.167.2",
        "@typescript-eslint/eslint-plugin": "^8.2.0",
        "@typescript-eslint/parser": "^8.2.0",
        "@vitejs/plugin-react": "^4.3.1",
        "autoprefixer": "^10.4.20",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "postcss": "^8.4.41",
        "tailwindcss": "^3.4.10",
        "ts-jest": "^29.2.4",
        "typescript": "^5.5.4"
    }
}
